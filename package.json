{
    "name": "@nordcom/nordy",
    "version": "0.1.0",
    "description": "Add source-control to your Discord server",
    "prettier": "@nordcom/prettier",
    "main": "./build/index.js",
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=18"
    },
    "scripts": {
        "prepare": "husky install",
        "generate": "npm-run-all generate:*",
        "generate:graphql": "graphql-codegen-esm -r ts-node/register --config codegen.ts",
        "start": "node --experimental-loader=extensionless build/index.js",
        "predev": "npm run generate",
        "dev": "tsc-watch --noClear --onSuccess \"npm run dev:start\"",
        "dev:start": "npm run postbuild && npm run generate && npm run start",
        "prebuild": "npm run generate",
        "build": "tsc --build",
        "postbuild": "tsc-alias",
        "clean": "tsc --build --clean && rm -rf build/",
        "lint": "npm-run-all --parallel lint:*",
        "lint:prettier": "prettier src/**/*.* --check",
        "lint:types": "tsc -noEmit",
        "test": "jest --coverage --passWithNoTests"
    },
    "author": {
        "name": "Nordcom Group Inc.",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam Sandstr√∂m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/nordy.git"
    },
    "keywords": [
        "discord"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NordcomInc/nordy/issues"
    },
    "homepage": "https://nordy.nordcom.io/",
    "extensionless": {
        "lookFor": [
            "js",
            "mjs",
            "cjs"
        ]
    },
    "devDependencies": {
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/typescript": "4.0.1",
        "@graphql-codegen/typescript-resolvers": "4.0.1",
        "@nordcom/prettier": "0.1.1",
        "@types/body-parser": "1.19.5",
        "@types/cors": "2.8.16",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.8",
        "@types/node": "20.8.10",
        "eslint": "8.53.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-github": "4.10.1",
        "eslint-plugin-jest": "27.6.0",
        "husky": "8.0.3",
        "jest": "29.7.0",
        "lint-staged": "15.0.2",
        "npm-run-all": "4.1.5",
        "prettier": "3.0.3",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
        "tsc-alias": "1.8.8",
        "tsc-watch": "6.0.4",
        "typescript": "5.2.2"
    },
    "dependencies": {
        "@apollo/server": "4.9.5",
        "@apollo/subgraph": "2.5.6",
        "body-parser": "1.20.2",
        "cors": "2.8.5",
        "discord.js": "14.13.0",
        "dotenv": "16.3.1",
        "express": "4.18.2",
        "extensionless": "1.7.3",
        "graphql": "16.8.1",
        "graphql-http": "1.22.0",
        "graphql-tag": "2.12.6",
        "mongoose": "8.0.0",
        "tslog": "4.9.2",
        "uuid": "9.0.1"
    }
}
