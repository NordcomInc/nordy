{
    "name": "@nordcom/nordy",
    "version": "0.1.0",
    "description": "",
    "prettier": "@nordcom/prettier",
    "main": "./build/index.js",
    "type": "module",
    "engines": {
        "npm": ">=8",
        "node": ">=18"
    },
    "scripts": {
        "prepare": "husky install",
        "start": "node --experimental-loader=extensionless build/index.js",
        "dev": "nodemon --watch './src/**' --ext 'ts,js,json' --exec \"npm run build && npm run start\" | pino-pretty -L 10",
        "build": "tsc",
        "lint": "npm-run-all --parallel lint:*",
        "lint:prettier": "prettier src/**/*.* --check",
        "lint:types": "tsc -noEmit",
        "test": "jest --coverage --passWithNoTests"
    },
    "author": {
        "name": "Nordcom Group Inc.",
        "email": "opensource@nordcom.io",
        "url": "https://nordcom.io/"
    },
    "contributors": [
        {
            "name": "Filiph Siitam Sandstr√∂m",
            "email": "filiph@nordcom.io",
            "url": "https://github.com/filiphsps/"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NordcomInc/nordy.git"
    },
    "keywords": [
        "discord"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NordcomInc/nordy/issues"
    },
    "homepage": "https://github.com/NordcomInc/nordy/#readme",
    "extensionless": {
        "lookFor": [
            "js",
            "mjs",
            "cjs"
        ]
    },
    "devDependencies": {
        "@nordcom/prettier": "0.1.1",
        "@types/jest": "29.5.4",
        "@types/node": "20.5.7",
        "eslint": "8.48.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-github": "4.10.0",
        "eslint-plugin-jest": "27.2.3",
        "husky": "8.0.3",
        "jest": "29.6.4",
        "lint-staged": "14.0.1",
        "nodemon": "3.0.1",
        "npm-run-all": "4.1.5",
        "pino-pretty": "10.2.0",
        "prettier": "3.0.3",
        "ts-jest": "29.1.1",
        "typescript": "5.2.2"
    },
    "dependencies": {
        "discord.js": "14.13.0",
        "dotenv": "16.3.1",
        "extensionless": "1.4.5",
        "pino": "8.15.0"
    }
}
